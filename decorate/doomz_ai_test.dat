ACTOR deer2017 : StrifeHumanoid 
{
  Health 100
  PainChance 256
  Speed 10
  Radius 20
  Height 56
  scale 2
  Monster
  +NEVERTARGET
  -COUNTKILL
  +NOSPLASHALERT
  +FLOORCLIP
  +JUSTHIT
  
  SeeSound "POSSant/sight"
  AttackSound "POSSant/attack"
  PainSound "POSSant/pain"
  DeathSound "POSSant/death"
  HitObituary "$OB_POSSANT" // "%o should have never picked a fight with a deer"
  States
  {
  Spawn:
 //  POSS A 1 A_Look
    // POSS A 20 A_Look
	deea A 1
	deea A 20 A_LookEx (0,  0, 250, 250, 0, "See")
	
	
	 deea AABBCCDD 5 A_Wander
    Loop
  See:
  
  deea a 3 //A_SetSpeed( 12)
   Goto Runaway
   
  Runaway:
    deea AABBCCDD 2 A_Wander
    deea A 0 A_LookEx (0,  0, 250, 250, 360, "evenfaster")
	deea A 0 A_LookEx (0,  0, 700, 700, 360, "Runaway")
    deea a 0 //A_SetSpeed( 8)
    Goto worried
 
 evenfaster:
    deea a 0 //A_SetSpeed( 20)
	goto Runaway
 
  worried:
	deea A 0 A_LookEx (0,  0, 499, 499, 360, "See")
	deea AABBCCDD 5 A_Wander
    Loop
 
 Pain:
    deea a 3
    deea a 3 A_Pain
	
    Goto evenfaster
	   
  Wound:
    deea a 5
    deea b 10 A_GetHurt
       Goto Wound
  Death:
    
    deea e 5 A_NoBlocking
  deea eeeee 1 A_SpawnItemEx("Splatspawner",0,0,random(4,8),random(0,2),random(0,2),random(0,3),random(-160,200))
    deea eee 1 A_SpawnItemEx("meatpickup",0,0,random(4,8),random(0,2),random(0,2),random(0,3),random(-160,200))
	zoss a 0 A_setscale(0.5,0.5)
    deea e 1400
    Stop
  XDeath:
	zoss a 0 A_setscale(0.3,0.3)
    zoss W -1 
    
  }
}




