actor usetest : SwitchableDecoration 27709
{
var int user_haspetrol;
   //$Category "PROPS/voxel"
   //$Title Table Lamp 4
   Radius 32
  Height 30
  scale 1.5
   Damage 3
  mass 99999999999999999
  health 70
  //PainChance 0
  + SHOOTABLE
 +COUNTKILL
 +NOBLOOD
