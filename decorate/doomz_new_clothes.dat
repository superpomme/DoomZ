Actor p_blue : CustomInventory 
{ 
	Inventory.PickupMessage "You put on a blue sued suit"
    Tag "knife"
	Inventory.PickupSound "misc/p_pkup"
	States
	  {
	  Spawn:
			klot A -1
			Stop
	   Pickup:
	       // TNT1 A 15 A_PlaySound("/sounds/scrunch.wav", CHAN_WEAPON)   THIS WORKED I THINK!
		TNT1 A 15 A_PlaySound("luggage", CHAN_WEAPON)  
		
		TNT1 A 0 ACS_Execute(24344,1)
			TNT1 A 0 A_GiveInventory("knife", 1)
	   vanish:
			TNT1 A 0 
			stop
	  }
}



