 actor redclothes : CustomInventory 
 //this one works but the p_blueclothes is wonky.
{
  Inventory.PickupMessage "clothes!!!"
  Inventory.PickupSound "misc/p_pkup"
  +COUNTITEM
  states
  {
  Spawn:
    KNFE A -1
    stop
  Pickup:
    TNT1 A 0 ACS_Execute(1333,1,random(0,2))
 
    stop
  }
}





Actor P_oldgreenclothes : CustomInventory 
{
  Inventory.PickupMessage "You picked up a hunk of cooked meat"
  Inventory.PickupSound "misc/p_pkup"
  health 20
  -solid
  -friendly
  scale 0.7

  States
  {
  Spawn:
		klot g 1

		loop 
   vanish:
		klot g 0 A_GiveInventory ("Stekfoo")
		klot g 0 A_TakeInventory ("Stekfoo")
				stop
  Pickup:
		TNT1 g 0 A_JumpIf(1 == (ACS_ExecuteWithResult(860,51,1)), "vanish") //51 is green clothes
		fail
  }
}

