ACTOR carxplod: rocket
{
  Game Doom
  SpawnID 127
  Radius 2
  Height 8
  Speed 80
  Damage 1
  Scale 3.0
  Projectile
  +RANDOMIZE
  +DEHEXPLOSION
  -ROCKETTRAIL
  -solid
  SeeSound "weapons/rocklx"
  DeathSound "weapons/rocklx"
  Obituary "$OB_MPROCKET" // "%o rode %k's rocket."
  States
  {
  Spawn:
    null A 2 Bright
    goto death 
	
  Death:
    MISL B 8 Bright A_Explode(150 ,800 ,0, true)
    MISL C 6 Bright
    MISL D 4 Bright
    Stop
  }
}







