	Obituary "$OB_ZOMBIE" // "%o was killed by a dayzombie."
    Translation "bluecoat"
  States
  { 
  //"80:102=198:204",
   Spawn:
   zoss A 1 
   
   
   zoss a 1  
  zoss AAABBCCDD 8 A_Wander
