Actor p_knife2 : CustomInventory  11111
{ 
	Inventory.PickupMessage "You picked up a knife"
    Tag "knife"
	Inventory.PickupSound "misc/p_pkup"
	States
	  {
	  Spawn:
			KNFE A -1
			Stop
	   Pickup:
			TNT1 A 0 A_JumpIf(1 == (ACS_ExecuteWithResult(8699,1)), "vanish")
			TNT1 A 0 A_GiveInventory("knife", 1)
	   vanish:
			TNT1 A 0 
			stop
	  }
}

 

Actor zcanteenpickup : CustomInventory 
{
  Inventory.PickupMessage "You picked up a full canteen thing."
  Inventory.PickupSound "misc/p_pkup"
 Tag "A full water canteen"
  scale 0.2
  States
  {
  Spawn:
		CANP A -1
		Stop
