Actor p_knife : CustomInventory 
{ 
	Inventory.PickupMessage "You picked up a knife"
    Tag "knife"
	Inventory.PickupSound "misc/p_pkup"
	States
	  {
	  Spawn:
			KNFE A -1
			Stop
	   Pickup:
	   
			TNT1 A 0 A_JumpIf(1 == (ACS_ExecuteWithResult(869,1)), "vanish")
			TNT1 A 0 A_GiveInventory("knife", 1)
	   vanish:
			TNT1 A 0 
			stop
	  }
}








